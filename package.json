{
  "name": "hnms-app",
  "version": "1.0.0",
  "description": "HNMS App",
  "private": true,
  "author": "Hieu Nguyen",
  "scripts": {
    "postinstall": "cd scripts && sh ./fix-lib.sh",
    "start": "react-native start",
    "eject": "rm -rf ios && rm -rf android && react-native eject",
    "lib:fix": "cd scripts && sh ./fix-lib.sh",
    "pod:install": "cd ios && pod install",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "open-xcode": "open -a Xcode ./ios/hnmsApp.xcworkspace",
    "android:debug-develop": "react-native run-android --no-jetifier --variant=developDebug --appId=hiunguyn.hnms.dev",
    "android:debug-staging": "react-native run-android --no-jetifier --variant=stagingDebug --appId=hiunguyn.hnms.stg",
    "android:debug-product": "react-native run-android --no-jetifier --variant=productDebug --appId=hiunguyn.hnms.prod",
    "android:release-develop": "react-native run-android --no-jetifier --variant=developRelease --appId=hiunguyn.hnms.dev",
    "android:release-staging": "react-native run-android --no-jetifier --variant=stagingRelease --appId=hiunguyn.hnms.stg",
    "android:release-product": "react-native run-android --no-jetifier --variant=productRelease --appId=hiunguyn.hnms.prod",
    "build:apk-develop": "rm -rf build && rm -rf android/build && export ENVFILE=.env.development && cd android && ./gradlew clean  && ./gradlew assembleDevelopRelease",
    "build:apk-staging": "rm -rf build && rm -rf android/build && export ENVFILE=.env.staging && cd android && ./gradlew clean  && ./gradlew assembleStagingRelease",
    "build:apk-product": "rm -rf build && rm -rf android/build && export ENVFILE=.env.production && cd android && ./gradlew clean && ./gradlew assembleProductRelease",
    "build:aab-product": "rm -rf build && rm -rf android/build && export ENVFILE=.env.production && cd android && ./gradlew clean && ./gradlew bundleProductRelease",
    "codepush:ios-develop": "appcenter codepush release-react -a <ownerName>/<appName> -d Development -t 1.0.0",
    "codepush:ios-staging": "appcenter codepush release-react -a <ownerName>/<appName> -d Staging -t 1.0.0",
    "codepush:ios-product": "appcenter codepush release-react -a HieuBeo/hnms-ios -d Production -t 1.0.0",
    "codepush:android-develop": "appcenter codepush release-react -a <ownerName>/<appName> -d Development -t 1.0.0",
    "codepush:android-staging": "appcenter codepush release-react -a <ownerName>/<appName> -d Staging -t 1.0.0",
    "codepush:android-product": "appcenter codepush release-react -a HieuBeo/hnms-android -d Production -t 1.0.0"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.8.2",
    "@react-native-async-storage/async-storage": "^1.15.6",
    "@react-native-community/datetimepicker": "^3.5.2",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/stack": "^6.0.7",
    "axios": "^0.21.1",
    "dayjs": "^1.10.6",
    "i18next": "^20.4.0",
    "lodash.debounce": "^4.0.8",
    "react": "17.0.2",
    "react-hook-form": "^7.18.0",
    "react-i18next": "^11.11.4",
    "react-native": "0.65.1",
    "react-native-camera": "^4.2.1",
    "react-native-code-push": "^7.0.4",
    "react-native-config": "^1.4.4",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-gifted-chat": "^0.16.3",
    "react-native-masked-text": "^1.13.0",
    "react-native-modal": "^13.0.0",
    "react-native-modalize": "^2.0.10",
    "react-native-paper": "^4.9.2",
    "react-native-permissions": "^3.2.0",
    "react-native-qrcode-scanner": "^1.5.4",
    "react-native-safe-area-context": "^3.3.0",
    "react-native-screens": "^3.5.0",
    "react-native-size-matters": "^0.4.0",
    "react-native-static-safe-area-insets": "^2.1.1",
    "react-native-url-polyfill": "^1.3.0",
    "react-query": "^3.21.0",
    "socket.io-client": "^4.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.15.4",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/cli": "^5.0.1",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/lodash.debounce": "^4.0.6",
    "@types/react-native": "^0.64.5",
    "@types/react-native-loading-spinner-overlay": "^0.5.3",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/eslint-plugin-tslint": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.4.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unused-imports": "^1.1.2",
    "metro-react-native-babel-preset": "^0.66.0",
    "prettier": "^2.3.2",
    "react-native-codegen": "^0.0.7",
    "react-native-eject": "^0.1.2",
    "tslint": "^6.1.3",
    "typescript": "^3.8.3"
  }
}
